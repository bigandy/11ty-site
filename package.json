{
	"name": "andrewhudson.dev",
	"version": "1.0.1",
	"description": "A personal blog built with Eleventy static site generator.",
	"scripts": {
		"dev": "npx eleventy --serve --quiet dev",
		"build": "npm run clean && ELEVENTY_ENV=production eleventy",
		"watch": "npx eleventy --watch",
		"debug": "DEBUG=* npx eleventy",
		"clean": "del-cli dist"
	},
	"author": {
		"name": "Andrew JD Hudson",
		"url": "https://andrewhudson.dev"
	},
	"license": "MIT",
	"devDependencies": {
		"@11ty/eleventy": "0.11.1",
		"@11ty/eleventy-plugin-rss": "1.0.9",
		"@11ty/eleventy-plugin-syntaxhighlight": "3.0.4",
		"cssnano": "4.1.10",
		"del-cli": "3.0.1",
		"luxon": "1.25.0",
		"markdown-it": "11.0.0",
		"markdown-it-anchor": "5.3.0",
		"postcss": "7.0.32",
		"postcss-color-mix": "1.1.0",
		"postcss-comment": "2.0.0",
		"postcss-import": "12.0.1",
		"postcss-mixins": "6.2.3",
		"postcss-simple-vars": "5.0.2",
		"precss": "4.0.0",
		"eleventy-plugin-pwa": "1.0.8"
	}
}
